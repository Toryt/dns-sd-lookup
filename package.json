{
  "name": "@toryt/dns-sd-lookup",
  "version": "1.1.9",
  "description": "Node library that looks up service instance definitions for a service type defined with DNS-SD (RFC 6763)",
  "main": "index.js",
  "keywords": [
    "registry",
    "service",
    "DNS",
    "DNS-SD",
    "PTR",
    "SRV",
    "TXT",
    "RFC 6763"
  ],
  "homepage": "https://github.com/Toryt/dns-sd-lookup",
  "repository": {
    "type": "git",
    "url": "https://github.com/Toryt/dns-sd-lookup.git"
  },
  "bugs": "https://github.com/Toryt/dns-sd-lookup/issues",
  "author": {
    "name": "Jan Dockx",
    "email": "jandockx@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=v6.11.3"
  },
  "dependencies": {
    "@toryt/contracts-iv": "^4.0.0",
    "deeper": "^2.1.0",
    "denodeify": "^1.2.1",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@ppwcode/terraform-ppwcode-modules": "^3.8.5",
    "cartesian": "^1.0.1",
    "codecov": "^3.0.1",
    "credits-cli": "^3.1.0",
    "legally": "^3.3.7",
    "mocha": "^5.1.1",
    "mocha-reporter": "^0.1.1",
    "must": "github:jandockx/js-must#fork/0.13.4/1",
    "npm-check": "^5.7.0",
    "nyc": "^13.1.0",
    "standard": "^12.0.1",
    "tldrlegal": "^1.0.6"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "html",
      "text"
    ]
  },
  "scripts": {
    "pretest": "npm-check -i mocha-reporter -i standard -i npm-check -i legally -i @ppwcode/terraform-ppwcode-modules -i codecov || exit 0",
    "test": "standard && nyc mocha \"test/**/*Test.js\"",
    "legalReport": "legally -l -r && tldrlegal",
    "reportCoverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "preversion": "npm run test",
    "postinstall": "npm run update_credits",
    "update_credits": "credits . --reporter markdown > CREDITS.md",
    "upgrade": "rm -Rf node_modules package-lock.json && npm install && npm test"
  }
}
