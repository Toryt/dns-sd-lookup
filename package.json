{
  "name": "@toryt/dns-sd-lookup",
  "version": "1.0.2",
  "description": "Node library that looks up service instance definitions for a service type defined with DNS-SD (RFC 6763)",
  "main": "index.js",
  "keywords": [
    "registry",
    "service",
    "DNS",
    "DNS-SD",
    "PTR",
    "SRV",
    "TXT",
    "RFC 6763"
  ],
  "homepage": "https://bitbucket.org/toryt/dns-sd-lookup",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/toryt/dns-sd-lookup.git"
  },
  "bugs": "https://bitbucket.org/toryt/dns-sd-lookup/issues",
  "author": {
    "name": "Jan Dockx",
    "email": "jandockx@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=v6.11.3"
  },
  "dependencies": {
    "@toryt/contracts-iii": "^3.1.0",
    "deeper": "^2.1.0",
    "denodeify": "^1.2.1",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "@ppwcode/terraform-ppwcode-modules": "^3.4.0",
    "cartesian": "^1.0.1",
    "codecov": "^2.3.0",
    "credits-cli": "^3.1.0",
    "legally": "^2.9.0",
    "mocha": "^3.5.0",
    "mocha-reporter": "^0.0.3",
    "must": "github:jandockx/js-must#fork/0.13.4/1",
    "npm-check": "^5.4.5",
    "nyc": "^11.2.1",
    "standard": "^10.0.3",
    "tldrlegal": "^1.0.5"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "html",
      "text"
    ]
  },
  "scripts": {
    "pretest": "npm-check -i mocha-reporter -i standard -i npm-check -i legally -i @ppwcode/terraform-ppwcode-modules -i codecov",
    "test": "standard && nyc mocha 'test/**/*Test.js'",
    "update_credits": "credits . --reporter markdown > CREDITS.md"
  }
}
